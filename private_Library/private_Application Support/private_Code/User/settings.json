  "vim.insertModeKeyBindings": [
    // escape insert mode
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    // change inner word remap
    {
      "before": [
        "<leader>",
        "<leader>",
        "<leader>"
      ],
      "after": [
        "c",
        "i",
        "w"
      ]
    },
    // change yyp: copy and paste this line on next bottom line
    {
      "before": [
        "<leader>",
        "<leader>",
        "n"
      ],
      "after": [
        "y",
        "y",
        "p"
      ]
    },
    // delete inner word
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "i",
        "w"
      ]
    },
    // visual insert paragraph
    {
      "before": [
        "<leader>",
        "a"
      ],
      "after": [
        "v",
        "i",
        "p"
      ]
    },
    // Move cursor to end line
    {
      "before": [
        "<leader>",
        "e"
      ],
      "after": [
        "$"
      ]
    },
    // Move cursor to start line
    {
      "before": [
        "<leader>",
        "s"
      ],
      "after": [
        "_"
      ]
    },
    // move to the paired bracket
    {
      "before": [
        "-"
      ],
      "after": [
        "%"
      ]
    },
    // yanking from cursor to end line
    {
      "before": [
        "Y"
      ],
      "after": [
        "y",
        "$"
      ]
    },
    // yanking from cursor to end line
    {
      "before": [
        "<leader>",
        "l",
      ],
      "after": [
        "y",
        "$"
      ]
    },
    // yanking from inner ()
    {
      "before": [
        "<Leader>",
        "("
      ],
      "after": [
        "y",
        "i",
        "("
      ]
    },
    // yanking from inner []
    {
      "before": [
        "<Leader>",
        "["
      ],
      "after": [
        "y",
        "i",
        "["
      ]
    },
    // add shortcut on ysiw`
    {
      "before": [
        "<Leader>",
        "`"
      ],
      "after": [
        "y",
        "s",
        "i",
        "w",
        "`"
      ]
    },
    // yanking from inner {}
    {
      "before": [
        "<Leader>",
        "]"
      ],
      "after": [
        "y",
        "i",
        "{"
      ]
    },
    // yanking from inner ''
    {
      "before": [
        "<Leader>",
        "'"
      ],
      "after": [
        "y",
        "i",
        "'"
      ]
    },
    // yanking from inner ""
    {
      "before": [
        "<Leader>",
        ";"
      ],
      "after": [
        "y",
        "i",
        "\""
      ]
    },
    // toogle comment
    {
      "before": [
        "m"
      ],
      "commands": [
        "editor.action.commentLine"
      ]
    },
    // :noh
    {
      "before": [
        "<Esc>"
      ],
      "commands": [
        ":noh"
      ]
    },
    // toogle workbench status bar and tab bar
    {
      "before": [
        "<Leader>",
        "h"
      ],
      "commands": [
        "workbench.action.toggleStatusbarVisibility",
        "workbench.action.toggleTabsVisibility"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "5",
        "j"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "5",
        "k"
      ]
    },
    {
      "before": [
        "<Leader>",
        "j"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "w"
      ]
    },
    {
      "before": [
        "<Leader>",
        "k"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "b"
      ]
    },
    {
      "before": [
        "<Leader>",
        "f"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "s"
      ]
    },
    {
      "before": [
        "<Leader>",
        "u"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "k"
      ]
    },
    {
      "before": [
        "<Leader>",
        ""
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "e"
      ]
    },
    {
      "before": [
        "<Leader>",
        "g",
        "e"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "g",
        "e"
      ]
    },
    {
      "before": [
        "<Leader>",
        "bdw"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "bdw"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "t"
      ]
    },
    // insert line break on cursor bottom
    {
      "before": [
        "<Leader>",
        "<Leader>",
        "l",
      ],
      "commands": [
        "lineBreakInsert"
      ],
      "silent": true
    },
    {
      "before": [
        "n"
      ],
      "commands": [
        "bookmarks.jumpToNext"
      ]
    },
    {
      "before": [
        "<leader>",
        "n"
      ],
      "commands": [
        "bookmarks.jumpToPrevious"
      ]
    }
  ],
