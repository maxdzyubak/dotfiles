  "vim.insertModeKeyBindings": [
    // escape insert mode
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    // move to the paired bracket
    {
      "before": [
        "-"
      ],
      "after": [
        "%"
      ]
    },
    // yanking from cursor to end line
    {
      "before": [
        "Y"
      ],
      "after": [
        "y",
        "$"
      ]
    },
    // yanking from inner ()
    {
      "before": [
        "<Leader>",
        "("
      ],
      "after": [
        "y",
        "i",
        "("
      ]
    },
    // yanking from inner []
    {
      "before": [
        "<Leader>",
        "["
      ],
      "after": [
        "y",
        "i",
        "["
      ]
    },
    // add shortcut on ysiw`
    {
      "before": [
        "<Leader>",
        "`"
      ],
      "after": [
        "y",
        "s",
        "i",
        "w",
        "`"
      ]
    },
    // yanking from inner {}
    {
      "before": [
        "<Leader>",
        "]"
      ],
      "after": [
        "y",
        "i",
        "{"
      ]
    },
    // yanking from inner ''
    {
      "before": [
        "<Leader>",
        "'"
      ],
      "after": [
        "y",
        "i",
        "'"
      ]
    },
    // yanking from inner ""
    {
      "before": [
        "<Leader>",
        ";"
      ],
      "after": [
        "y",
        "i",
        "\""
      ]
    },
    // toogle comment
    {
      "before": [
        "m"
      ],
      "commands": [
        "editor.action.commentLine"
      ]
    },
    {
      "before": [
        "<Esc>"
      ],
      "commands": [
        ":noh"
      ]
    },
    {
      "before": [
        "<Leader>",
        "s"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "5",
        "j"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "5",
        "k"
      ]
    },
    {
      "before": [
        "<Leader>",
        "j"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "w"
      ]
    },
    {
      "before": [
        "<Leader>",
        "k"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "b"
      ]
    },
    {
      "before": [
        "<Leader>",
        "e"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "b"
      ]
    },
    {
      "before": [
        "<Leader>",
        "f"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "s"
      ]
    },
    {
      "before": [
        "<Leader>",
        "u"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "k"
      ]
    },
    {
      "before": [
        "<Leader>",
        ""
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "e"
      ]
    },
    {
      "before": [
        "<Leader>",
        "g",
        "e"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "g",
        "e"
      ]
    },
    {
      "before": [
        "<Leader>",
        "bdw"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "bdw"
      ]
    },
    {
      "before": [
        "<Leader>",
        "F"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "F"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t"
      ],
      "after": [
        "<Leader>",
        "<Leader>",
        "t"
      ]
    },
    {
      "before": [
        "<Leader>",
        "p"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<Leader>",
        "<Leader>",
        "j"
      ],
      "commands": [
        "lineBreakInsert"
      ],
      "silent": true
    },
    {
      "before": [
        "n"
      ],
      "commands": [
        "bookmarks.jumpToNext"
      ]
    },
    {
      "before": [
        "<leader>",
        "l"
      ],
      "commands": [
        "bookmarks.list"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "commands": [
        "bookmarks.clear"
      ]
    },
    {
      "before": [
        "<leader>",
        "n"
      ],
      "commands": [
        "bookmarks.jumpToPrevious"
      ]
    }
  ],
