{
  "editor.detectIdentation": false,
  "editor.fontWeight": 400,
  "editor.fontSize": 16,
  "editor.lineHeight": 1.8,
  "editor.fontLigatures": true,
  "editor.fontFamily": "JetBrainsMono NerdFont, JetBrains Mono, Fira Code,  SF Mono, Martian Mono, Source Code Pro, Menlo, Monaco, monospace",
  "editor.rulers": [78],
  "editor.lineNumbers": "off",
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.renderLineHighlight": "line",
  "editor.foldingHighlight": false,
  "editor.wordWrapColumn": 78,
  "editor.wordWrap": "wordWrapColumn",
  "editor.scrollbar.horizontal": "hidden",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.autoClosingOvertype": "auto",
  "editor.cursorStyle": "line",
  "editor.links": true,
  "editor.hover.enabled": false,
  "editor.semanticHighlighting.enabled": true,
  "liveServer.settings.port": 0,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": true, // => navigation on top
  "editor.renderControlCharacters": true,
  "files.defaultLanguage": "javascript",
  "git.ignoreMissingGitWarning": true,
  "editor.detectIndentation": false,
  "editor.folding": true,
  "editor.lightbulb.enabled": false,
  "editor.glyphMargin": false,
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": 400,
  "editor.renderWhitespace": "none",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "security.workspace.trust.enabled": false,
  "editor.linkedEditing": true,
  "editor.stickyScroll.enabled": true, // => visible details by scroll
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?-",
  "editor.suggest.showClasses": true,
  "editor.lineDecorationsWidth": 0,
  "editor.codeLens": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.multiCursorModifier": "ctrlCmd",

  "vim.easymotion": true,
  "vim.leader": "<space>",
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "editor.codeActionsOnSave": {},
  "editor.cursorBlinking": "solid",
  "workbench.iconTheme": "vscode-great-icons",
  "editor.suggest.showKeywords": false,
  "editor.inlayHints.enabled": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.editor.languageDetectionHints": {
    "untitledEditors": false,
    "notebookEditors": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "npm.keybindingsChangedWarningShown": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 0,
  "editor.tabSize": 2,
  "emmet.triggerExpansionOnTab": true,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "open-in-browser.default": "Chrome",
  "window.menuBarVisibility": "toogle",
  "window.openFilesInNewWindow": "off",
  "window.openFoldersInNewWindow": "off",
  "window.newWindowDimensions": "maximized",
  "window.commandCenter": true,
  "explorer.enableDragAndDrop": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.autoReveal": false,
  "explorer.openEditors.visible": 0,
  "explorer.excludeGitIgnore": true,
  "extensions.ignoreRecommendations": false,
  "html.autoClosingTags": true,
  "gitlens.telemetry.enabled": false,
  "debug.node.autoAttach": "disabled",
  "scm.diffDecorations": "none",
  "vsicons.presets.foldersAllDefaultIcon": true,
  "vsicons.dontShowNewVersionMessage": true,
  "keyboard.dispatch": "keyCode",
  "security.workspace.trusc.untrustedFiles": "open",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.untitled.hint": "text",
  "[python]": {
    "editor.formatOnType": true,
    "editor.tabSize": 4
  },
  "htmlhint.enable": false,
  "htmlhint.documentSelector": ["html", "htm"],
  "htmlhint.options": {
    "tagname-lowercase": true,
    "attr-lowercase": true,
    "attr-value-double-quotes": true,
    "doctype-first": true,
    "tag-pair": true,
    "spec-char-escape": true,
    "id-unique": true,
    "src-not-empty": true,
    "attr-no-duplication": true,
    "title-require": true
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#FFAC00",
    "editorBracketHighlight.foreground2": "#00C9FF",
    "editorBracketHighlight.foreground3": "#BB00FF",
    "editorBracketHighlight.foreground4": "#00FF6D",
    "editorBracketHighlight.foreground5": "#366AFF",
    "editorBracketHighlight.foreground6": "#D32D72",
    "editorBracketHighlight.unexpectedBracket.foreground": "#b5b5b5"
  },

  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.suggest.showFolders": true,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.quickSuggestionsDelay": 10,
  "editor.suggestOnTriggerCharacters": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "recentlyUsed",
  "editor.suggest.preview": false,
  "editor.suggest.showVariables": true,
  "editor.wordBasedSuggestions": true,
  "editor.parameterHints.enabled": false,
  "editor.parameterHints.cycle": false,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.snippetSuggestions": "none",
  "html.suggest.html5": false,
  "javascript.suggest.enabled": false,
  "javascript.inlayHints.enumMemberValues.enabled": false,
  "editor.suggest.showProperties": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "erb",
    "html",
    "css"
  ],
  "xi.lookupPath": "~/hub/",
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "editor.accessibilitySupport": "off",
  "ecsstractor_port.add_comment": false,
  "ecsstractor_port.empty_line_before_nested_selector": false,
  "editor.bracketPairColorization.enabled": true,
  "workbench.colorTheme": "grigoryvp memory theme",
  "editor.tabCompletion": "on",
  "vsintellicode.modify.editor.suggestSelection": "disabled",
  "gitlens.advanced.blame.customArguments": [],
  "gitlens.currentLine.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "diffEditor.wordWrap": "off",
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false
}
